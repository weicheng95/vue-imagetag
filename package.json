{
  "name": "vue-imagetag",
  "description": "this is a vue component able to let you tag on image",
  "version": "1.0.5",
  "main": "dist/vue-imagetag.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --dest dist/ --target lib --name vue-imagetag ./src/components/vue-imagetag.vue",
    "lint": "vue-cli-service lint",
    "demo:build": "vue-cli-service build"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "v-tooltip": "^2.0.2",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-svg-loader": "^0.12.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "keywords": [
    "image-tagging",
    "tagonimage",
    "vue",
    "components"
  ],
  "author": "WeiCheng",
  "license": "ISC"
}